<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="200dp">


            <ImageView
                    android:src="@drawable/no_profile"
                    android:layout_width="71dp"
                    android:layout_height="72dp"
                    android:id="@+id/imageView" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHorizontal_bias="0.024" app:layout_constraintVertical_bias="0.012"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/profile_firstname" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toEndOf="@+id/imageView" android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.04"
                    app:layout_constraintVertical_bias="0.071" android:textStyle="bold"
                    android:textAlignment="viewStart" android:textSize="16sp"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/profile_secondname" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.071"
                    android:textStyle="bold"
                    android:textAlignment="viewStart"
                    app:layout_constraintStart_toEndOf="@+id/profile_firstname"
                    android:layout_marginStart="6dp" app:layout_constraintHorizontal_bias="0.0"
                    android:textSize="16sp"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/profile_country"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.248"
                    app:layout_constraintStart_toEndOf="@+id/imageView"
                    android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.04"/>

            <TextView
                    android:text="TextView"
                    android:layout_width="80dp"
                    android:layout_height="35dp"
                    android:id="@+id/followers" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:textAlignment="textEnd"
                    app:layout_constraintEnd_toStartOf="@+id/imageView2" android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/profile_save"
                    app:layout_constraintVertical_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/imageView4"
                    android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.172"/>
            <ImageView
                    android:src="@drawable/followers"
                    android:layout_width="23dp"
                    android:layout_height="18dp"
                    android:id="@+id/imageView4" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.4" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/edit_profile"
                    app:layout_constraintVertical_bias="0.993"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="81dp"
                    android:layout_height="35dp"
                    android:id="@+id/followings" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="72dp"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/edit_profile"
                    app:layout_constraintVertical_bias="0.992" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/imageView4"
                    app:layout_constraintHorizontal_bias="1.0"
                    android:textAlignment="textEnd"/>
            <ImageView
                    android:src="@drawable/followings"
                    android:layout_width="23dp"
                    android:layout_height="18dp"
                    android:id="@+id/imageView2" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintHorizontal_bias="0.79"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/profile_save"
                    app:layout_constraintVertical_bias="1.0"/>

            <android.support.design.button.MaterialButton
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="153dp"
                    android:layout_height="wrap_content"
                    android:text="Complete profile"
                    android:textColor="@android:color/background_dark"
                    android:textSize="14sp"
                    app:backgroundTint="@android:color/transparent"
                    app:cornerRadius="8dp"
                    app:rippleColor="#33AAAAAA"
                    android:id="@+id/profile_save"
                    app:strokeColor="@android:color/darker_gray"
                    app:strokeWidth="1dp" tools:ignore="DuplicateIds,MissingConstraints"
                    android:textAllCaps="false"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
                    android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.966"
                    app:layout_constraintVertical_bias="0.94"/>

            <android.support.design.button.MaterialButton
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="145dp"
                    android:layout_height="wrap_content"
                    android:text="Edit profile"
                    android:textColor="@android:color/black"
                    android:textSize="14sp"
                    android:id="@+id/edit_profile"
                    app:backgroundTint="@android:color/transparent"
                    app:cornerRadius="8dp"
                    app:rippleColor="#33AAAAAA"
                    app:strokeColor="@android:color/darker_gray"
                    app:strokeWidth="1dp" tools:ignore="MissingConstraints"
                    android:textAllCaps="false"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
                    android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toStartOf="@+id/profile_save"
                    app:layout_constraintHorizontal_bias="0.481"
                    app:layout_constraintVertical_bias="0.94"/>

        </android.support.constraint.ConstraintLayout>


        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="200dp">


            <TextView
                    android:text="Proflile information:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/textView"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/profie_listview" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.029"
                    app:layout_constraintVertical_bias="0.0"/>
            <ListView
                    android:layout_width="0dp"
                    android:id="@+id/profie_listview"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_height="157dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintVertical_bias="0.703"/>


        </android.support.constraint.ConstraintLayout>


        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="430dp" android:id="@+id/my_videotab">
            <TextView
                    android:text="History"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/history"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.183"
                    android:textColor="@android:color/black" android:textSize="14sp"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="44dp"/>
            <TextView
                    android:text="Saved Videos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/saved_videos"
                    android:textColor="@android:color/black" android:textSize="14sp"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="88dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.206"/>
            <TextView
                    android:text="My videos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:id="@+id/myvideotab"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.193"
                    android:textColor="@android:color/black" android:textSize="14sp"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="132dp"/>
            <TextView
                    android:text="Liked Videos"
                    android:layout_width="wrap_content"
                    android:layout_height="19dp"
                    android:id="@+id/liked_videos"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintHorizontal_bias="0.203"
                    android:textColor="@android:color/black"
                    android:textSize="14sp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="180dp"/>
            <ImageView
                    android:src="@drawable/history"
                    android:layout_width="28dp"
                    android:layout_height="21dp"
                    android:id="@+id/imageView8" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.021"
                    app:layout_constraintVertical_bias="0.091"/>
            <ImageView
                    android:src="@drawable/saved_video"
                    android:layout_width="25dp"
                    android:layout_height="24dp"
                    android:id="@+id/imageView10" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.027"
                    android:layout_marginTop="84dp" app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                    android:src="@drawable/my_videos"
                    android:layout_width="32dp"
                    android:layout_height="25dp"
                    android:id="@+id/imageView12" android:layout_marginTop="128dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.022"/>
            <ImageView
                    android:src="@drawable/liked_videos"
                    android:layout_width="26dp"
                    android:layout_height="24dp"
                    android:id="@+id/imageView13" android:layout_marginTop="176dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.027"/>
            <TextView
                    android:text="Library:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView12" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.023"
                    android:layout_marginTop="4dp" app:layout_constraintTop_toTopOf="parent"/>


        </android.support.constraint.ConstraintLayout>
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>